<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AgrToastTitle">
        <item name="android:textAppearance">@style/BodySmallBold</item>
        <item name="android:textColor">@color/black_font</item>
    </style>
    <style name="AgrToastMessage">
        <item name="android:textAppearance">@style/CaptionLargeRegular</item>
        <item name="android:textColor">@color/black_font</item>
    </style>
    <style name="AgrToastTextAction">
        <item name="android:textAppearance">@style/BodyLargeBold</item>
        <item name="android:textSize">@dimen/_14sp</item>
    </style>
    <style name="AgrToastTextAction.Primary">
        <item name="android:textColor">@color/agr_normal</item>
    </style>
    <style name="AgrToastTextAction.White">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="AgrToastTextAction.BlackFont">
        <item name="android:textColor">@color/black_font</item>
    </style>
</resources>